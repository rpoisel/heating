/*
 * This is a logi.CAD 3 template for the local PLC object
 */

{
	CHANNEL LocalChannel
		TCP
			ADDRESS := 127.0.0.1;
			PORT    := 1534;
		END_TCP
	END_CHANNEL
}

CONFIGURATION LocalConfiguration
	RESOURCE local ON LinuxX86 { ON_CHANNEL := LocalChannel }
		TASK DefaultTask(INTERVAL := TIME#500ms, PRIORITY := 38229);
		PROGRAM TemplateInstance WITH DefaultTask :
			Heating;
	END_RESOURCE
END_CONFIGURATION
